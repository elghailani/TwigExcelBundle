{
    "name": "elghailani/twig-excel-bundle",
    "type": "symfony-component",
    "description": "This Symfony bundle provides a PhpExcel integration for Twig",
    "keywords": ["excel","phpexcel","twig","symfony"],
    "homepage": "https://github.com/elghailani/TwigExcelBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdellah EL GHAILANI",
            "email": "a.elghailani@mail.com"
        },
        {
            "name": "Community Contributors",
            "homepage": "https://github.com/elghailani/TwigExcelBundle/graphs/contributors"
        }
    ],
    "require": {
        "php": ">7.0",
        "symfony/framework-bundle": ">4.0",
        "phpoffice/phpspreadsheet": ">1.8.1"
    },
    "require-dev": {
        "symfony/symfony": ">4.0",
        "symfony/phpunit-bridge": ">3.0",
        "sensio/framework-extra-bundle": ">3.0",
        "mpdf/mpdf": ">6.0"
    },
    "minimum-stability": "dev",
    "suggest": {
        "mpdf/mpdf": "Add support for PDF rendering, requires ~6.0"
    },
    "autoload": {
        "psr-4": { "MewesK\\TwigExcelBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
